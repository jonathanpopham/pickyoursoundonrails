    <script class="cssdeck">
        (function() {
            var canvas = document.querySelector('#paint');
        	var ctx = canvas.getContext('2d');
        	
        	var sketch = document.querySelector('#sketch');
        	var sketch_style = getComputedStyle(sketch);
        	canvas.width = parseInt(sketch_style.getPropertyValue('width'));
        	canvas.height = parseInt(sketch_style.getPropertyValue('height'));
            
            var imageLoader = document.getElementById('imageLoader');
            imageLoader.addEventListener('change', handleImage, false);
            
        	var mouse = {x: 0, y: 0};
        	
            var checkoutdata = document.querySelector('#imagedata');
            
        	/* Mouse Capturing Work */
        	canvas.addEventListener('mousemove', function(e) {
        		mouse.x = e.pageX - this.offsetLeft;
        		mouse.y = e.pageY - this.offsetTop;
        	}, false);
        	
        	/* Drawing on Paint App */
        	ctx.lineWidth = 5;
        	ctx.lineJoin = 'round';
        	ctx.lineCap = 'round';
        	ctx.strokeStyle = 'black';
        	 
            
        	canvas.addEventListener('mousedown', function(e) {
        			ctx.beginPath();
        			ctx.moveTo(mouse.x, mouse.y);
        	 
        			canvas.addEventListener('mousemove', onPaint, false);
        	}, false);
        	 
        	canvas.addEventListener('mouseup', function() {
        			canvas.removeEventListener('mousemove', onPaint, false);
                    checkoutdata.value = canvas.toDataURL('png');
        	}, false);
            
        	/// todo http://paperjs.org/examples/smoothing/
        	var onPaint = function() {
                    var athickslide = document.querySelector('#thicknessrange');
            		var athickpen = document.querySelector('#thicknespen');
            		var adepthishpen = parseInt(athickpen.value); 
        		    var adepthish = 255 - parseInt(athickslide.value);
                    ctx.strokeStyle =  "#"+adepthish.toString(16)+adepthish.toString(16)+adepthish.toString(16);
        			ctx.lineWidth = adepthishpen;
        			ctx.lineTo(mouse.x, mouse.y);
        			ctx.stroke();
        	};
        	
            var pngdown = document.querySelector('#downloadpng');
            pngdown.onclick = function()
                {
                     window.open(canvas.toDataURL('png'));    
                }
           
            function handleImage(e){
                var reader = new FileReader();
                reader.onload = function(event){
                    var img = new Image();
                    img.onload = function(){
                        canvas.width = img.width;
                        canvas.height = img.height;
                        ctx.drawImage(img,0,0);
                    }
                    img.src = event.target.result;
                }
                reader.readAsDataURL(e.target.files[0]);     
            }
            
            
            var imageObj = new Image();

            imageObj.onload = function() {
              canvas.width = imageObj.width;
              canvas.height = imageObj.height;
              ctx.drawImage(imageObj, 0, 0);
            };
            imageObj.src = 'assets/img/samplesrealsize.png';
            
        }());
    </script>
